{
  "name": "@esm-bundle/angular__router",
  "version": "9.1.13",
  "description": "ESM and SystemJS bundles for @angular/router",
  "type": "module",
  "scripts": {
    "test": "yarn build && concurrently -n w: 'yarn:test:*'",
    "test:browser": "karma start karma.conf.cjs --single-run",
    "debug:test:browser": "karma start karma.conf.cjs",
    "test:unit": "mocha -r chai/register-expect 'node-test/**/*.*'",
    "test:formatting": "prettier --check './**/*'",
    "build": "rollup -c",
    "prettier": "prettier --write './**/*'",
    "release": "release-it",
    "prepublishOnly": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn test"
    }
  },
  "files": [
    "esm",
    "system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-bundle/angular__router.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esm-bundle/angular__router/issues"
  },
  "homepage": "https://github.com/esm-bundle/angular__router#readme",
  "devDependencies": {
    "@angular/common": "npm:@esm-bundle/angular__common@9.1.3-fix.0",
    "@angular/core": "npm:@esm-bundle/angular__core@9.1.3-fix.0",
    "@angular/router": "11.1.1",
    "esm-bundle-scripts": "1.2.0",
    "rxjs": "npm:@esm-bundle/rxjs@6.5.5-fix.1"
  }
}
